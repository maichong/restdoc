[ignore]
.*/build/.*
.*/lib/.*
.*/.history/.*
.*/node_modules/.*
.*/flow-declarations/.*

[libs]
node_modules/flow-declarations/restdoc
node_modules/flow-declarations/lodash
node_modules/flow-declarations/marked

[options]
include_warnings=true
suppress_comment= \\(.\\|\n\\)*\\$Flow
esproposal.decorators=ignore
esproposal.export_star_as=enable
